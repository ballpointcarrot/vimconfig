[user]
	name = Christopher Kruse
	email = kruse.christopher@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	autocrlf = input
	safecrlf = true
	editor = vim
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an] \" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
